//Function to draw a single 8-bit pixel
var drawPixel = function(x, y, color){
    for(var i = 0; i < pixel; i++){
        for(var a = 0; a < pixel; a++){
            set(x*pixel+a, y*pixel+i, color);
        }
    }
};

//Draws a single 16x16 pixel tile from pixel data
var drawTile = function(x, y, sData){
    for(var a = 0; a < 16; a++){
        for(var i = 0; i < 16; i++){
            var colors = sData[i+(a*16)];
            if(i+x+1 < 0 || i+x+1 > screenSize/pixel){
            } else if(colors === -1){
                //Draws a transparent pixel (somehow this works idk)
                drawPixel(i+x, a+y, color(69, 420, 1337, 0));
            } else{
                drawPixel(i+x, a+y, palette[colors]);
            }
        }
    }
};

