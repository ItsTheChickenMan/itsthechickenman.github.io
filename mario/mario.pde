//Contains x, y, size, speed, acceleration, current jump force, jump height, gravity, scroll, and other bits of data.
//Player class also contains 3 functions: collide, input, and draw (explained in more detail as they occur below)
var Player = function(){
    //Display variables
    this.size = 1;
    this.playerRunLeft = [sprites[32], sprites[33], sprites[34], sprites[33]];
    this.playerRunRight = [sprites[35], sprites[36], sprites[37], sprites[36]];
    this.playerState = "neutralRight";
    this.neutralState = "neutralRight";
    this.run = 0;
    
    //Position variables
    this.x = 64;
    this.y = pixel*208;
    this.scroll = 0;
    
    //Jump variables
    this.jumpForce = 0;
    this.jumpHeight = -8.2;  //-8.2 (4 tiles)
    this.gravity = 0.25; //0.25
    
    //Speed variables
    this.speed = 0;
    this.baseSpeed = 1.7;
    this.accel = 0.05;
    
    if(test){
        this.levels = testLevels;
    } else {
        this.levels = levels;
    }
    this.data = [];
    
    this.debug = [];
    //Method that detects collisions (left, right, up, or down) and corrects x or y if the user will go into an object (it's really messy and I might rework it later)
    this.collide = function(direction){
        var activePanelOffset = 0; 
        var activePanel = floor(((this.scroll+128+(tile/2))+(floor(this.x/2)-screenSize/4)+activePanelOffset)/256);
        var isCollide = false;
        var data = [];
        if(activePanel !== 0){
            for(var i = 0; i < 17; i++){
                this.data[i*18] = this.levels[activePanel-1][(i*16)+15];
            }
        }
        if(activePanel !== this.levels.length-1){
            for(var i = 1; i < 17; i++){
                this.data[(i*18)-1] = this.levels[activePanel+1][(i-1)*16];
            }
        }
        for(var i = 0; i < 16; i++){
            for(var a = 1; a < 17; a++){
                this.data[(i*18)+a] = this.levels[activePanel][(i*16)+(a-1)];
            }
        }
        var data = this.data;
        var increase = this.accel;
        for(var i = 0; i < data.length; i++){
            var dat;
            if(data[i] !== undefined){
                dat = data[i][0] ? data[i][0] : data[i];
            } else {
                dat = data[i];
            }
            if(dat !== 0){
                for(var a = 0; a < collidables.length; a++){
                    if(dat === collidables[a]){
                        var posX = ((i%18)*tile-((this.scroll)-activePanel*256)*pixel)-tile;
                        var posY = floor(i/18)*tile;
                        fill(255, 255, 255);
                        //rect(posX, posY, tile, tile);
                        if(direction === "left"){
                            if(this.x+(this.speed-increase)*pixel <= posX+tile && this.x+tile+(this.speed-increase)*pixel >= posX){
                                if((this.y >= posY && this.y < posY+tile) || (this.y+tile > posY && this.y+tile <= posY+tile)){
                                    isCollide = true;
                                    this.x = posX+tile;
                                    //println("left");
                                }
                                if(this.size === 1){
                                    if((this.y-tile >= posY && this.y-tile < posY+tile) || (this.y > posY && this.y <= posY+tile)){
                                            isCollide = true;
                                            this.x = posX+tile;
                                        }
                                    }
                                }
                        } else if(direction === "right"){
                            if(this.x+tile+(this.speed+increase)*pixel >= posX && this.x+(this.speed+increase)*pixel <= posX+tile){
                                if((this.y >= posY && this.y < posY+tile) || (this.y+tile > posY && this.y+tile <= posY+tile)){
                                    isCollide = true;
                                    this.x = posX-tile;
                                }
                                if(this.size === 1){
                                    if((this.y-tile >= posY && this.y-tile < posY+tile) || (this.y > posY && this.y <= posY+tile)){
                                        isCollide = true;
                                        this.x = posX-tile;
                                    }
                                }
                            }
                        } else if(direction === "up"){
                            var dif = this.size === 1 ? tile : 0;
                            if(this.y-dif+this.jumpForce+this.gravity <= posY+tile && this.y-dif >= posY){
                                if((this.x >= posX && this.x < posX+tile) || (this.x+tile > posX && this.x+tile <= posX+tile)){
                                    isCollide = true;
                                    this.y = posY+(pixel*32)+dif;
                                    this.jumpForce = -this.jumpForce;
                                }
                            }
                        } else if(direction === "down"){
                            if(this.y+tile+this.jumpForce+this.gravity >= posY && this.y < posY+tile){
                                if((this.x >= posX && this.x < posX+tile) || (this.x+tile > posX && this.x+tile <= posX+tile)){
                                    isCollide = true;
                                    this.jumpForce = 0;
                                    this.y = posY-tile;
                                }
                            }
                        }
                    }
                }
            }
        }
        return isCollide;
    };
    //Handles input
    this.input = function(){
        if(keys[37]){
            if(this.speed > 0){
                this.playerState = "driftLeft";
                if(!this.collide("right")){
                    this.speed -= this.accel*3;
                } else {
                    this.speed = 0;
                }
            } else {
                if(!this.collide("left")){
                    this.neutralState = "neutralLeft";
                    this.playerState = "runLeft";
                    this.speed -= this.accel;
                    if(keys[88]){
                        this.speed = constrain(this.speed, -this.baseSpeed*1.5, this.baseSpeed*1.5);
                    } else {
                        this.speed = constrain(this.speed, -this.baseSpeed, this.baseSpeed);
                    }
                } else {
                    this.playerState = this.neutralState;
                    this.run = 0;
                    this.speed = 0;
                }
            }
        } else if(keys[39]){
            if(this.speed < 0){
                this.playerState = "driftRight";
                if(!this.collide("left")){
                    this.speed += this.accel*3;
                } else {
                    this.speed = 0;
                }
            } else {
                if(!this.collide("right")){
                    this.neutralState = "neutralRight";
                    this.playerState = "runRight";
                    this.speed += this.accel;
                    if(keys[88]){
                        this.speed = constrain(this.speed, -this.baseSpeed*1.5, this.baseSpeed*1.5);
                    } else {
                        this.speed = constrain(this.speed, -this.baseSpeed, this.baseSpeed);
                    }
                } else {
                    this.playerState = this.neutralState;
                    this.run = 0;
                    this.speed = 0;
                }
            }
        } else {
            this.playerState = this.neutralState;
            this.run = 0;
            this.speed = 0;
        }
        if(!this.collide("down")){
            this.jumpForce += this.gravity;
            if(this.neutralState === "neutralLeft"){
                this.playerState = "jumpLeft";
            } else {
                this.playerState = "jumpRight";
            }
        } else if(keys[90] && !this.collide("up") && jump === false){
            jump = true;
            this.jumpForce = this.jumpHeight;
            if(this.neutralState === "neutralLeft"){
                this.playerState = "jumpLeft";
            } else {
                this.playerState = "jumpRight";
            }
        }
        if(!this.collide("up")){
            this.y += this.jumpForce;
        }
    };
    //Draws mario based on size and playerState
    this.draw = function() {
        if(this.size === 0){
            switch(this.playerState){
                case "neutralLeft":
                    image(sprites[30], this.x, this.y);
                    break;
                case "neutralRight":
                    image(sprites[31], this.x, this.y);
                    break;
                case "runLeft":
                        image(this.playerRunLeft[floor(this.run/(6+this.speed))%4], this.x, this.y);
                        this.run++;
                    break;
                case "runRight":
                        image(this.playerRunRight[floor(this.run/(6-this.speed))%4], this.x, this.y);
                        this.run++;
                    break;
                case "jumpLeft":
                    image(sprites[38], this.x, this.y);
                    break;
                case "jumpRight":
                    image(sprites[39], this.x, this.y);
                    break;
                case "driftLeft":
                    image(sprites[65], this.x, this.y);
                    break;
                case "driftRight":
                    image(sprites[64], this.x, this.y);
                    break;
                case "duck":
                    break;
                default:
                    image(sprites[18], this.x, this.y);
                    break;
                
            }
        } else if(this.size === 1){
            switch(this.playerState){
                case "neutralLeft":
                    image(sprites[41], this.x, this.y-tile);
                    image(sprites[43], this.x, this.y);
                    break;
                case "neutralRight":
                    image(sprites[42], this.x, this.y-tile);
                    image(sprites[43], this.x, this.y);
                    break;
                case "runLeft":
                    image(sprites[(floor(this.run/(6+this.speed))%3)+53], this.x, this.y);
                    image(sprites[(floor(this.run/(6+this.speed))%3)+50], this.x, this.y-tile);
                    this.run++;
                    break;
                case "runRight":
                    image(sprites[(floor(this.run/(6-this.speed))%3)+47], this.x, this.y);
                    image(sprites[(floor(this.run/(6-this.speed))%3)+44], this.x, this.y-tile);
                    this.run++;
                    break;
                case "jumpLeft":
                    image(sprites[58], this.x, this.y-tile);
                    image(sprites[59], this.x, this.y);
                    break;
                case "jumpRight":
                    image(sprites[56], this.x, this.y-tile);
                    image(sprites[57], this.x, this.y);
                    break;
                case "driftLeft":
                    image(sprites[69], this.x, this.y);
                    image(sprites[68], this.x, this.y-tile);
                    break;
                case "driftRight":
                    image(sprites[67], this.x, this.y);
                    image(sprites[66], this.x, this.y-tile);
                    break;
                case "duck":
                    if(this.neutralState === "neutralLeft"){
                        image(sprites[63], this.x, this.y); 
                        image(sprites[62], this.x, this.y-tile);
                    } else {
                        image(sprites[61], this.x, this.y);
                        image(sprites[60], this.x, this.y-tile);
                    }
                    break;
                default:
                    image(sprites[18], this.x, this.y);
                    image(sprites[18], this.x, this.y-tile);
                    break;
            }
        }
        this.input();
        if(this.x >= screenSize/2-tile && this.speed > 0){
            this.scroll += this.speed;
        } else {
            this.x += this.speed*pixel;
        }
    };
};

