var Panel = function(levelData){
    this.levelData = levelData;
    this.draw = function(x) {
        for(var i = 0; i < 16; i++){
            for(var a = 0; a < 16; a++){
                if(a*16+x < -16 || a*16+x > screenSize/pixel || this.levelData[i*16+a] === 0){
                
                } else {
                    if(levelData[(i*16+a)] > sprites.length - 1 || levelData[(i*16+a)] < 0){
                        image(sprites[18], (a*16+x)*pixel, (i*16)*pixel);
                    } else {
                        image(sprites[levelData[i*16+a]], (a*16+x)*pixel, (i*16)*pixel);
                    }
                }
            }
        }
    };
};

var drawLevel = function(player, theSimpsons){
    for(var f = 0; f < theSimpsons.length; f++){
        if((-player.scroll + f*256)+256 < 0 || -player.scroll+f*256 > screenSize/pixel){
        } else {
            theSimpsons[f].draw(-player.scroll + f*256);
        }
    }
};

var Player = function(){
    //this.playerRunLeft = [PlayerRunningLeft1, PlayerRunningLeft2, PlayerRunningLeft3, PlayerRunningLeft2];
    //this.playerRunRight = [PlayerRunningRight1, PlayerRunningRight2, PlayerRunningRight3, PlayerRunningRight2];
    this.playerRunLeft = [sprites[32], sprites[33], sprites[34], sprites[33]];
    this.playerRunRight = [sprites[35], sprites[36], sprites[37], sprites[36]];
    this.playerRunLeftBig = [];
    this.playerRunRightBig = [];
    this.x = 64;
    this.size = 0;
    this.y = pixel*208-(this.size*32);
    this.scroll = 0;
    this.playerState = "neutralRight";
    this.run = 0;
    this.speed = 0;
    this.neutralState = "neutralRight";
    this.jumpForce = 10;
    this.baseForce = 18.5;
    this.gravity = 10;
    this.baseSpeed = 1.7;
    this.accel = 0.05;
    this.collide = function(direction){
        var activePanel = floor(((this.scroll+128)+(floor(this.x/2)-112))/256);
        var isCollide = false;
        var data = levels[activePanel];
        var increase = 0.2;
        if(this.size+1){
            for(var i = 0; i < data.length; i++){
                if(data[i] !== 0){
                    for(var a = 0; a < collidables.length; a++){
                        if(data[i] === collidables[a]){
                            var posX = (i%16)*(pixel*16)-((this.scroll)-activePanel*256)*pixel;
                            var posY = floor(i/16)*(pixel*16);
                            //rect(posX, posY, 32, 32);
                            if(direction === "left"){
                                if(this.x+(this.speed-increase)*pixel <= posX+(pixel*16) && this.x+(pixel*16)+(this.speed-increase)*pixel >= posX){
                                    if((this.y >= posY && this.y < posY+(pixel*16)) || (this.y+(pixel*16) > posY && this.y+(pixel*16) <= posY+(pixel*16))){
                                        isCollide = true;
                                        this.x = posX+(pixel*16);
                                    }
                                }
                            } else if(direction === "right"){
                                if(this.x+(pixel*16)+(this.speed+increase)*pixel >= posX && this.x+(this.speed+increase)*pixel <= posX+(pixel*16)){
                                    if((this.y >= posY && this.y < posY+(pixel*16)) || (this.y+(pixel*16) > posY && this.y+(pixel*16) <= posY+(pixel*16))){
                                        isCollide = true;
                                        this.x = posX-(pixel*16);
                                    }
                                }
                            } else if(direction === "up"){
                                if(this.y <= posY+(pixel*16)+(this.jumpForce-this.gravity-0.25) && this.y >= posY){
                                    if((this.x >= posX && this.x < posX+(pixel*16)) || (this.x+(pixel*16) > posX && this.x+(pixel*16) <= posX+(pixel*16))){
                                        isCollide = true;
                                        this.y = posY+(pixel*32);
                                        //this.jumpForce = 6;
                                    }
                                }
                            } else if(direction === "down"){
                                if(this.y+(pixel*16)-(this.jumpForce-this.gravity-0.25) >= posY && this.y <= posY+(pixel*16)){
                                    if((this.x >= posX && this.x < posX+(pixel*16)) || (this.x+(pixel*16) > posX && this.x+(pixel*16) <= posX+(pixel*16))){
                                        isCollide = true;
                                        this.y = posY-(pixel*16);
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        return isCollide;
    };
    this.input = function(panels){
        if(keys[37] && !this.collide("left")){
            if(this.speed > 0){
                if(!this.collide("right")){
                    this.speed -= this.accel*3;
                } else {
                    this.speed = 0;
                }
            } else {
                this.speed -= this.accel;
            }
            if(keys[88]){
                this.speed = constrain(this.speed, -this.baseSpeed*1.5, this.baseSpeed*1.5);
                this.baseForce = 19.5;
            } else {
                this.speed = constrain(this.speed, -this.baseSpeed, this.baseSpeed);
                this.baseForce = 18.5;
            }
            if(this.speed > 0){
                this.playerState = "driftLeft";
            } else {
                this.neutralState = "neutralLeft";
                if(keys[90]){
                    this.playerState = "jumpLeft";
                    
                } else {
                    this.playerState = "runLeft";
                }
            }
            if(this.x >= 224 && this.speed > 0){
                this.scroll += this.speed;
            } else {
                this.x += this.speed*pixel;
            }
        } else if(keys[39] && !this.collide("right")){
            if(this.speed < 0){
                if(!this.collide("left")){
                    this.speed += this.accel*3;
                } else {
                    this.speed = 0;
                }
            } else {
                this.speed += this.accel;
            }
            if(keys[88]){
                this.speed = constrain(this.speed, -this.baseSpeed*1.5, this.baseSpeed*1.5);
                this.baseForce = 19.5;
            } else {
                this.speed = constrain(this.speed, -this.baseSpeed, this.baseSpeed);
                this.baseForce = 18.5;
            }
            if(this.speed < 0){
                this.playerState = "driftRight";
            } else {
                this.neutralState = "neutralRight";
                if(keys[90]){
                    this.playerState = "jumpRight";
                } else {
                    this.playerState = "runRight";
                }
            }
            if(this.x < 224 || this.scroll > 3120){
                this.x += this.speed*pixel;
            } else {
                this.scroll += this.speed;
            }
        } else if(keys[40]){
            if(this.size === 1){
                this.playerState = "duck";
            }
        } else {
            this.playerState = this.neutralState;
            this.run = 0;
            this.speed = 0;
            this.baseForce = 18.5;
        }
        if(keys[90]){
            if(this.collide("down")){
                this.jumpForce = this.baseForce;
            }
        }
        if(!this.collide("down") && !this.collide("up")){
            this.jumpForce -= 0.25;
            this.y -= this.jumpForce - this.gravity;
            if(this.neutralState === "neutralLeft"){
                this.playerState = "jumpLeft";
            } else {
                this.playerState = "jumpRight";
            }
        } else if(!this.collide("up")){
            this.jumpForce = 6;
        }
    };
    this.draw = function() {
        if(this.size === 0){
            switch(this.playerState){
                case "neutralLeft":
                    image(sprites[30], this.x, this.y);
                    break;
                case "neutralRight":
                    image(sprites[31], this.x, this.y);
                    break;
                case "runLeft":
                        image(this.playerRunLeft[floor(this.run/(6+this.speed))%4], this.x, this.y);
                        this.run++;
                    break;
                case "runRight":
                        image(this.playerRunRight[floor(this.run/(6-this.speed))%4], this.x, this.y);
                        this.run++;
                    break;
                case "jumpLeft":
                    image(sprites[38], this.x, this.y);
                    break;
                case "jumpRight":
                    image(sprites[39], this.x, this.y);
                    break;
                case "driftLeft":
                    image(sprites[32], this.x, this.y);
                    break;
                case "driftRight":
                    image(sprites[35], this.x, this.y);
                    break;
                case "duck":
                    break;
                
            }
        } else if(this.size === 1){
            switch(this.playerState){
                case "neutralLeft":
                    image(sprites[41], this.x, this.y-(pixel*16));
                    image(sprites[43], this.x, this.y);
                    break;
                case "neutralRight":
                    image(sprites[42], this.x, this.y-(pixel*16));
                    image(sprites[43], this.x, this.y);
                    break;
                case "runLeft":
                    break;
                case "runRight":
                    break;
                case "jumpLeft":
                    break;
                case "jumpRight":
                    break;
                case "driftLeft":
                    break;
                case "driftRight":
                    break;
                case "duck":
                    break;
                default:
                    break;
            }
        }
    };
};

