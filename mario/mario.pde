var drawPixel = function(x, y, color){
    if(get(x*pixel, y*pixel) === color){
        //If the pixel it's about to set is the same color as it's going to set, don't bother rendering
    } else {
        set(x*pixel, y*pixel, color);
        set(x*pixel+1, y*pixel, color);
        set(x*pixel, y*pixel+1, color);
        set(x*pixel+1, y*pixel+1, color);
    }
};

//Draws a single 16x16 pixel tile from pixel data
var drawTile = function(x, y, sData){
    for(var a = 0; a < 16; a++){
        for(var i = 0; i < 16; i++){
            var colors = sData[i+(a*16)];
            if(i+x+1 < 0 || i+x+1 > screenSize/pixel){
            } else {
                drawPixel(i+x, a+y, palette[colors]);
            }
        }
    }
};

